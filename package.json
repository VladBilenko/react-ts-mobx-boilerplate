{
  "name": "react-ts-mobx-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "npm-run-all -p fix start-js",
    "build": "npm-run-all build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "lint": "tslint --project . --fix",
    "sass-lint": "sass-lint --config ./sass-lint.yml -v -q",
    "sass-lint-fix": "sass-lint-auto-fix --config ./sass-lint.yml",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma=es5 --print-width 120 --write \"./**/*.ts\" \"./**/*.tsx\"",
    "fix": "npm-run-all prettier sass-lint-fix",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "commit-hook": "git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix && npm run lint && npm run sass-lint && npm run commit-hook"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.0",
    "typescript": "^3.7.5",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
